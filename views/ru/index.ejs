                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="blog-section section-padding">
            <div class="section-heading">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="section-title wow animate__animated animate__fadeInUp">
                            <span class="sub-title">Последние новости</span>
                            <h2>Последние новости о нашей клинике</h2>
                            <div class="section-title-separator"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row pt-100">
                    <% for(const blog of blogs) { %>
                        <div class="col-lg-4 col-md-6 d-flex  wow animate__animated animate__fadeInUp">
                            <div class="blog-item">
                                <div class="blog-image">
                                    <a href="/blog/<%=blog._id%>"><img class="img-fluid"
                                            src="/blogs/<%=blog.photo%>.<%=blog.file%>" alt="Post Image"></a>
                                </div>
                                <div class="blog-content">
                                    <ul class="entry-meta meta-item">
                                        <li>
                                            <div class="post-author">
                                                <a href="/" class="mb-0"><span>Ofiyat Med</span></a>
                                            </div>
                                        </li>
                                        <% const options={ year: 'numeric' , month: 'long' , day: 'numeric' }; %>
                                            <li class="date-icon"><i class="fal fa-calendar-alt"></i>
                                                <%= (new Date(blog.date)).toLocaleDateString('en-US', options) %>
                                            </li>
                                    </ul>
                                    <h3 class="blog-title"><a href="/blog/<%=blog._id%>">
                                            <%=blog.title%>
                                        </a></h3>
                                    <p class="blog-description">
                                        <%=blog.simpleDescription%>
                                    </p>
                                    <p class="viewlink"><a href="/blog/<%=blog._id%>">Подробнее <i
                                                class="fal fa-long-arrow-right"></i></a></p>
                                </div>
                            </div>
                        </div>
                        <% } %>
                </div>
            </div>
        </section>
        <section class="subscribe-section section-padding theme-overlay"
            style="background-image: url(/images/slider/slider_img_4.jpg); background-size: cover;">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-12 col-md-10 col-lg-6">
                        <div class="section-title text-center white">
                            <h3 class="text-white wow animate__animated animate__fadeInUp">Подписывайтесь На Нашу
                                Рассылку
                            </h3>
                            <p class="text-white wow animate__animated animate__fadeInUp">Оставьте свой адрес
                                электронной почты, чтобы быть в курсе наших новостей и рекламных акций</p>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-center">
                    <div class="col-12 col-md-10 col-lg-6 wow animate__animated animate__fadeInUp">
                        <form id="subscribe-form" action="/" method="post">
                            <div class="input-group mb-3">
                                <input type="email" name="email" class="form-control field-subscribe"
                                    placeholder="Введите адрес электронной почты." required />
                            </div>
                            <div class="subscribe-btn text-center">
                                <button type="submit" class="button-one mb-5">Подписаться</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
        <%- include('./partials/footer.ejs')%>
            <script src="/vendor/jquery/jquery.min.js"></script>
            <script src="/vendor/waypoints/jquery-waypoints.min.js"></script>
            <script src="/vendor/bootstrap/bootstrap.min.js"></script>
            <script src="/vendor/wow/wow.js"></script>
            <script src="/vendor/jquery-counterup/jquery-counterup.min.js"></script>
            <script src="/vendor/venobox/venobox.min.js"></script>
            <script src="/vendor/owl-carousel/owl-carousel.min.js"></script>
            <script src="/js/main.js"></script>
</body>

</html>